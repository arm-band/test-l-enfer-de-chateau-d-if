<%- include('partial/_util') -%>
<%
    parameters = {
        title: 'Home',
        description: 'JavaScript で互いにオブジェクトを要素に持つ配列2つを比較して、重複していない要素のみを取り出すサンプルです。',
        ogp: {
            card: 'photo',
            account: 'Bredtn_1et',
            type: 'website',
            image: 'https://upload.wikimedia.org/wikipedia/commons/2/2f/L\'Estaque_et_le_Ch%C3%A2teau_d\'If,_par_Paul_C%C3%A9zanne.jpg'
        },
        errFlag: false
    }
-%>
<%- include('partial/_head', { parameters }) -%>
<body>
<%- include('partial/_header') -%>
<main class="l-main">
    <section class="container py-5">
        <h1><i class="fa-solid fa-fw fa-chess-rook me-2" aria-hidden="true"></i><%= parameters.title %></h1>
        <p><%= parameters.description %></p>
    </section>
    <section class="container pb-5">
        <h2>Origins</h2>
        <div class="row">
            <div class="col-12 col-md-6">
                <textarea name="origin" id="origin" rows="10" class="w-100"></textarea>
            </div>
            <div class="col-12 col-md-6">
                <textarea name="selected" id="selected" rows="10" class="w-100"></textarea>
            </div>
        </div>
    </section>
    <section class="container pb-5">
        <h2>Diff</h2>
        <div class="row">
            <div class="col-12 col-md-6">
                <textarea name="diff" id="diff" rows="10" class="w-100"></textarea>
            </div>
        </div>
    </section>
    <section class="container pb-5">
        <h2>Only label</h2>
        <div class="row">
            <div class="col-12 col-md-6">
                <textarea name="originLabel" id="originLabel" rows="10" class="w-100"></textarea>
            </div>
            <div class="col-12 col-md-6">
                <textarea name="selectedLabel" id="selectedLabel" rows="10" class="w-100"></textarea>
            </div>
        </div>
    </section>
    <section class="container pb-5">
        <h2>Diff Label</h2>
        <div class="row">
            <div class="col-12 col-md-6">
                <textarea name="diffLabel" id="diffLabel" rows="10" class="w-100"></textarea>
            </div>
        </div>
    </section>
</main>
<%- include('partial/_footer') -%>
<%- include('partial/_js_loader') -%>
</body>
</html>